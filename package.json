  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js",
      "src/client/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "report-dir": "./docs/server-coverage"
  }
