    "react-test-renderer": "16.0.0",
    "run-sequence": "2.2.0",
    "sassdoc": "2.3.0",
    "selenium-standalone": "6.9.0",
    "sinon": "4.0.0",
    "sinon-chai": "2.13.0",
    "stylelint": "8.1.1",
    "stylelint-config-standard": "17.0.0",
    "stylelint-scss": "2.1.0",
